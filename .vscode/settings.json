{
    "eslint.autoFixOnSave": true,
    // "tslint.autoFixOnSave": [
    //     "array-type",
    //     "arrow-parens",
    //     "indent",
    //     "member-ordering",
    //     "no-consecutive-blank-lines",
    //     "no-var-keyword",
    //     "object-literal-key-quotes",
    //     "one-variable-per-declaration",
    //     "ordered-imports",
    //     "prefer-const",
    //     "quotemark",
    //     "semicolon",
    //     "space-before-function-paren",
    //     "trailing-comma",
    //     "eofline"
    // ],
    "tslint.jsEnable": true,
    // "tslint.packageManager": "yarn",
    // "tslint.run": "onType",
    "files.eol": "\n",
    "javascript.preferences.quoteStyle": "single",
    "typescript.preferences.quoteStyle": "single",
    "editor.insertSpaces": false,
    "search.exclude": {
        "**/node_modules": true,
        "**/bower_components": true,
        "built/**/*": true,
        "yarn.lock": true,
        "yarn-error.log": true,
        "samples": true
    },
    "editor.codeActionsOnSave": {
        "source.organizeImports": false
    },
    "typescript.tsdk": "node_modules\\typescript\\lib",
    "cSpell.words": [
        "Comparator",
        "DEVTOOLS",
        "Deserializer",
        "Hashable",
        "ICVC",
        "Scrollable",
        "Seqs",
        "Shamu",
        "Signup",
        "Unmount",
        "adenflorian",
        "adsr",
        "automerge",
        "autoplay",
        "autoprefixer",
        "autosize",
        "callout",
        "contenteditable",
        "desaturate",
        "deserialize",
        "draggable",
        "focusable",
        "fucky",
        "hashbow",
        "immer",
        "input",
        "keyframes",
        "localstorage",
        "lowpass",
        "nginx",
        "notroot",
        "onmidimessage",
        "onstatechange",
        "pageview",
        "polyfill",
        "reselect",
        "selectable",
        "signups",
        "slidedown",
        "slidein",
        "slideout",
        "slideup",
        "soundbank",
        "sysex",
        "tabindex",
        "typesafe",
        "unpress",
        "unselectable",
        "uuidv",
        "wucky"
    ],
    "editor.dragAndDrop": false,
    "cSpell.ignorePaths": [
        "**/package-lock.json",
        "**/node_modules/**",
        "**/vscode-extension/**",
        "**/.git/**",
        ".vscode",
        "typings",
        "built/**/*",
        ".cache",
        "TODO.md"
    ],
    "less.lint.emptyRules": "ignore",
    "codemetrics.basics.CodeLensHiddenUnder": 20,
}
