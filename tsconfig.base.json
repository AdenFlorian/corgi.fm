{
	"compilerOptions": {
		"removeComments": true,
		"noErrorTruncation": true,
		"noEmitOnError": true,
		"noFallthroughCasesInSwitch": true,
		"noEmit": true,
		"allowSyntheticDefaultImports": true,
		"noUnusedLocals": false,
		"noUnusedParameters": false,
		"noImplicitReturns": true,
		"strict": true,
		"experimentalDecorators": true,
		"sourceMap": true,
		"module": "commonjs",
		"types": [
			"jest"
		]
	}
}
