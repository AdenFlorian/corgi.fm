{
  "name": "sha-mu",
  "version": "0.0.1",
  "main": "server.js",
  "license": "UNLICENSED",
  "scripts": {
    "start": "yarn build && concurrently --names \"build,server\" \"yarn watch\" \"nodemon --inspect built/server/server.js\"",
    "watch": "watch \"yarn build\" src",
    "start-prod": "pm2 start server/server.js",
    "stop-prod": "pm2 stop server/server.js",
    "lint": "eslint .",
    "test": "mocha",
    "tdd": "mocha --watch --reporter dot",
    "clean": "gulp clean",
    "build": "gulp build",
    "deploy": "yarn build && bash scripts/deploy.sh",
    "setup-prod-server": "bash scripts/setup-prod-server.sh"
  },
  "dependencies": {
    "@types/gulp": "^4.0.5",
    "@types/react": "^16.3.16",
    "@types/react-dom": "^16.0.6",
    "@types/socket.io": "^1.4.34",
    "express": "^4.16.3",
    "react": "^16.4.0",
    "react-dom": "^16.4.0",
    "react-redux": "^5.0.7",
    "redux": "^4.0.0",
    "socket.io": "^2.1.1"
  },
  "devDependencies": {
    "concurrently": "^3.5.1",
    "eslint": "^4.19.1",
    "gulp": "^4.0.0",
    "gulp-clean": "^0.4.0",
    "gulp-sourcemaps": "^2.6.4",
    "gulp-typescript": "^4.0.2",
    "mocha": "^5.2.0",
    "nodemon": "^1.17.5",
    "pm2": "^2.10.4",
    "typescript": "^2.9.1",
    "watch": "^1.0.2"
  }
}
