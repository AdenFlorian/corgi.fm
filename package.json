{
  "name": "sha-mu",
  "version": "0.0.1",
  "main": "server.js",
  "license": "UNLICENSED",
  "scripts": {
    "start": "yarn build && concurrently --names \"parcel,build,server\" \"yarn watch-parcel\" \"yarn watch\" \"yarn watch-server\"",
    "watch": "watch \"yarn build\" src/server",
    "watch-parcel": "parcel watch src/client/index.html --out-dir built/client",
    "watch-server": "nodemon --watch built/server built/server/server.js",
    "start-prod": "pm2 start server/server.js",
    "stop-prod": "pm2 stop server/server.js",
    "lint": "eslint . && tslint src/**/*.ts*",
    "test": "mocha",
    "tdd": "mocha --watch --reporter dot",
    "clean": "gulp clean",
    "build": "gulp build",
    "deploy": "yarn build && bash scripts/deploy.sh",
    "setup-prod-server": "bash scripts/setup-prod-server.sh"
  },
  "dependencies": {
    "express": "^4.16.3",
    "prop-types": "^15.6.1",
    "react": "^16.4.0",
    "react-dom": "^16.4.0",
    "react-redux": "^5.0.7",
    "redux": "^4.0.0",
    "socket.io": "^2.1.1"
  },
  "devDependencies": {
    "@types/express": "^4.16.0",
    "@types/gulp": "^4.0.5",
    "@types/react": "^16.3.16",
    "@types/react-dom": "^16.0.6",
    "@types/react-redux": "^6.0.2",
    "@types/redux": "^3.6.0",
    "@types/socket.io": "^1.4.34",
    "concurrently": "^3.5.1",
    "eslint": "^4.19.1",
    "gulp": "^4.0.0",
    "gulp-clean": "^0.4.0",
    "gulp-sourcemaps": "^2.6.4",
    "gulp-typescript": "^4.0.2",
    "mocha": "^5.2.0",
    "nodemon": "^1.17.5",
    "parcel-bundler": "^1.8.1",
    "pm2": "^2.10.4",
    "tslint": "^5.10.0",
    "typescript": "^2.9.1",
    "watch": "^1.0.2"
  }
}
