{
  "name": "corgi.fm",
  "version": "0.3.16",
  "main": "server.js",
  "license": "UNLICENSED",
  "scripts": {
    "start": "yarn clean && concurrently --names \"parcel,server\" \"yarn watch-parcel\" \"yarn watch-server\"",
    "watch-parcel": "parcel src/client/index.html",
    "watch-server": "ts-node-dev --respawn --transpileOnly --notify false --inspect --project src/server/tsconfig.json src/server/server.ts",
    "start-prod": "NODE_ENV=production pm2 start server/server.js",
    "start-prod-docker": "NODE_ENV=production pm2-runtime built/server/server.js",
    "lint": "eslint . && tslint src/client/**/*.ts* --project src/client/tsconfig.json && tslint src/server/**/*.ts* --project src/server/tsconfig.json && tslint src/common/**/*.ts* --project src/common/tsconfig.json ",
    "test": "yarn test-client && yarn test-server && yarn test-common",
    "test-client": "cross-env TS_NODE_PROJECT=\"src/client/tsconfig.json\" mocha --opts src/client/mocha.opts",
    "tdd-client": "cross-env TS_NODE_PROJECT=\"src/client/tsconfig.json\" mocha --opts src/client/mocha.opts --watch --watch-extensions ts",
    "test-server": "cross-env TS_NODE_PROJECT=\"src/server/tsconfig.json\" mocha --opts src/server/mocha.opts",
    "test-common": "cross-env TS_NODE_PROJECT=\"src/common/tsconfig.json\" mocha --opts src/common/mocha.opts",
    "test-debug": "mocha --inspect --opts src/mocha.opts",
    "tdd": "mocha --watch src/**/*.ts* --reporter dot --opts src/mocha.opts",
    "clean": "rm -rf .cache built dist yarn-error.log",
    "deploy-test": "yarn version --patch && yarn build && bash scripts/deploy-test.sh",
    "deploy-prod": "yarn version --patch && yarn build && bash scripts/deploy-prod.sh",
    "build": "rm -rf built && yarn build-server && yarn build-client && yarn build-other",
    "build-client": "parcel build src/client/index.html src/client/newsletter.html --out-dir built/public --no-source-maps",
    "build-server": "tsc --project src/server/tsconfig.json --outDir built --noEmit false",
    "build-other": "cp package.json yarn.lock built && cp src/public/* built/public",
    "build-docker": "docker build -t corgi.fm .",
    "start-docker": "yarn build-docker && docker run -p 3000:3000 corgi.fm",
    "setup-test-server": "bash scripts/setup-test-server.sh",
    "setup-prod-server": "bash scripts/setup-prod-server.sh"
  },
  "dependencies": {
    "adsr": "^1.0.1",
    "animal-id": "^0.0.1",
    "autoprefixer": "^9.4.2",
    "babel-polyfill": "^6.26.0",
    "color": "^3.0.0",
    "color-hash": "^1.0.3",
    "common-tags": "^1.8.0",
    "cors": "^2.8.5",
    "express": "^4.16.3",
    "file-saver": "^2.0.0",
    "immutable": "^4.0.0-rc.12",
    "lodash": "^4.17.11",
    "midi-writer-js": "^1.6.0",
    "pkginfo": "^0.4.1",
    "prop-types": "^15.6.1",
    "query-string": "^6.1.0",
    "rc-slider": "^8.6.1",
    "react": "^16.4.0",
    "react-contextmenu": "^2.11.0",
    "react-dom": "^16.4.0",
    "react-draggable": "^3.2.1",
    "react-ga": "^2.5.6",
    "react-hot-loader": "^4.8.2",
    "react-icons": "^3.3.0",
    "react-input-autosize": "^2.2.1",
    "react-redux": "^5.0.7",
    "react-svg": "^7.2.2",
    "redux": "^4.0.0",
    "redux-devtools-extension": "^2.13.7",
    "remote-redux-devtools": "^0.5.14",
    "reselect": "^3.0.1",
    "socket.io": "^2.1.1",
    "soundbank-reverb": "^1.1.2",
    "typesafe-actions": "^3.0.0",
    "uuid": "^3.3.2",
    "victor": "^1.1.0"
  },
  "devDependencies": {
    "@types/chai": "^4.1.7",
    "@types/color": "^3.0.0",
    "@types/common-tags": "^1.8.0",
    "@types/cors": "^2.8.4",
    "@types/express": "^4.16.0",
    "@types/file-saver": "^2.0.0",
    "@types/lodash": "^4.14.119",
    "@types/mocha": "^5.2.5",
    "@types/react": "^16.3.16",
    "@types/react-dom": "^16.0.6",
    "@types/react-input-autosize": "^2.0.0",
    "@types/react-redux": "^6.0.2",
    "@types/redux": "^3.6.0",
    "@types/remote-redux-devtools": "^0.5.3",
    "@types/sinon": "^7.0.0",
    "@types/socket.io": "^2.1.2",
    "@types/socket.io-client": "^1.4.32",
    "@types/tone": "https://github.com/Tonejs/TypeScript.git",
    "@types/uuid": "^3.4.4",
    "@types/victor": "^1.1.0",
    "chai": "^4.1.2",
    "concurrently": "^3.5.1",
    "cross-env": "^5.1.6",
    "del": "^3.0.0",
    "eslint": "^4.19.1",
    "eslint-plugin-react-hooks": "^1.6.0",
    "less": "^3.0.4",
    "mocha": "^5.2.0",
    "parcel-bundler": "^1.11.0",
    "pm2": "^2.10.4",
    "sinon": "^7.2.2",
    "ts-node": "^6.1.0",
    "ts-node-dev": "^1.0.0-pre.26",
    "tslint": "^5.13.0",
    "tslint-immutable": "^5.3.0",
    "typescript": "^3.3.1",
    "typescript-tslint-plugin": "^0.3.1"
  }
}
