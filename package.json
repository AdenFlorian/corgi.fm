{
  "name": "corgi.fm",
  "version": "0.4.40",
  "main": "server.js",
  "license": "UNLICENSED",
  "scripts": {
    "start": "yarn clean && concurrently --names \"parcel,server\" \"yarn watch-parcel\" \"yarn watch-server\"",
    "watch-parcel": "parcel src/client/index.html",
    "watch-server": "ts-node-dev --respawn --notify false --project src/server/tsconfig.json src/server/server.ts",
    "watch-ts-client": "tsc --project src/client/tsconfig.json --noEmit --watch",
    "watch-server-old": "ts-node-dev --respawn --transpileOnly --notify false --inspect --project src/server/tsconfig.json src/server/server.ts",
    "start-test": "pm2 start server/ecosystem.config.js --env test",
    "start-prod": "pm2 start server/ecosystem.config.js --env prod",
    "lint": "eslint . && tslint src/client/**/*.ts* --project src/client/tsconfig.json && tslint src/server/**/*.ts* --project src/server/tsconfig.json && tslint src/common/**/*.ts* --project src/common/tsconfig.json ",
    "test": "jest",
    "clean": "rm -rf .cache built dist yarn-error.log && jest --clearCache",
    "deploy-test": "yarn version --patch && yarn build && bash scripts/deploy-test.sh",
    "deploy-prod": "yarn version --patch && yarn build && bash scripts/deploy-prod.sh",
    "build": "rm -rf built && yarn build-server && yarn build-client && yarn build-other",
    "build-client": "parcel build src/client/index.html --out-dir built/public --no-source-maps",
    "build-server": "tsc --project src/server/tsconfig.json --outDir built --noEmit false && cp src/server/ecosystem.config.js built/server",
    "build-other": "cp package.json yarn.lock built && cp src/public/* built/public",
    "setup-test-server": "bash scripts/setup-test-server.sh",
    "setup-prod-server": "bash scripts/setup-prod-server.sh"
  },
  "jest": {
    "projects": [
      {
        "displayName": "client",
        "preset": "ts-jest",
        "testEnvironment": "jsdom",
        "rootDir": "src/client"
      },
      {
        "displayName": "common",
        "preset": "ts-jest",
        "testEnvironment": "node",
        "rootDir": "src/common"
      },
      {
        "displayName": "server",
        "preset": "ts-jest",
        "testEnvironment": "node",
        "rootDir": "src/server"
      }
    ]
  },
  "dependencies": {
    "@sentry/browser": "^5.5.0",
    "@sentry/integrations": "^5.5.0",
    "@sentry/node": "5.5.0",
    "animal-id": "^0.0.1",
    "autoprefixer": "^9.4.2",
    "babel-polyfill": "^6.26.0",
    "color": "^3.0.0",
    "common-tags": "^1.8.0",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "file-saver": "^2.0.0",
    "firebase": "^6.2.4",
    "immutable": "^4.0.0-rc.12",
    "lodash": "^4.17.11",
    "midi-writer-js": "^1.6.0",
    "moment": "^2.24.0",
    "mongodb": "^3.2.7",
    "pkginfo": "^0.4.1",
    "prop-types": "^15.6.1",
    "react": "^16.8.6",
    "react-contextmenu": "^2.11.0",
    "react-dom": "npm:@hot-loader/react-dom",
    "react-draggable": "^3.2.1",
    "react-ga": "^2.5.6",
    "react-hot-loader": "^4.8.3",
    "react-icons": "^3.3.0",
    "react-input-autosize": "^2.2.1",
    "react-redux": "^7.1.0",
    "react-svg": "^7.2.2",
    "redux": "^4.0.0",
    "redux-devtools-extension": "^2.13.7",
    "remote-redux-devtools": "^0.5.14",
    "reselect": "^3.0.1",
    "socket.io": "^2.1.1",
    "soundbank-reverb": "^1.1.2",
    "typesafe-actions": "^3.0.0",
    "uuid": "^3.3.2",
    "victor": "^1.1.0"
  },
  "devDependencies": {
    "@types/chai": "^4.1.7",
    "@types/color": "^3.0.0",
    "@types/common-tags": "^1.8.0",
    "@types/cors": "^2.8.4",
    "@types/express": "^4.16.0",
    "@types/file-saver": "^2.0.0",
    "@types/jest": "^24.0.15",
    "@types/lodash": "^4.14.119",
    "@types/moment": "^2.13.0",
    "@types/mongodb": "^3.1.28",
    "@types/mongodb-memory-server": "^1.8.0",
    "@types/react": "^16.3.16",
    "@types/react-dom": "^16.0.6",
    "@types/react-input-autosize": "^2.0.0",
    "@types/react-redux": "^7.1.1",
    "@types/redux": "^3.6.0",
    "@types/remote-redux-devtools": "^0.5.3",
    "@types/sinon": "^7.0.0",
    "@types/socket.io": "^2.1.2",
    "@types/socket.io-client": "^1.4.32",
    "@types/supertest": "^2.0.7",
    "@types/uuid": "^3.4.4",
    "@types/victor": "^1.1.0",
    "chai": "^4.1.2",
    "concurrently": "^3.5.1",
    "eslint": "^4.19.1",
    "eslint-plugin-react-hooks": "^1.6.0",
    "jest": "^24.8.0",
    "less": "^3.0.4",
    "mongodb-memory-server-global": "^5.1.5",
    "parcel-bundler": "^1.12.3",
    "pm2": "^2.10.4",
    "sinon": "^7.2.2",
    "supertest": "^4.0.2",
    "ts-jest": "^24.0.2",
    "ts-node": "^6.1.0",
    "ts-node-dev": "^1.0.0-pre.26",
    "tslint": "^5.13.0",
    "tslint-immutable": "^5.3.0",
    "tslint-react": "^4.0.0",
    "tslint-react-hooks": "^2.1.1",
    "typescript": "^3.3.1",
    "typescript-tslint-plugin": "^0.3.1"
  }
}
